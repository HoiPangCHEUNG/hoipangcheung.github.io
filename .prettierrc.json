{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "none",
  "attributeGroups": [
    "^(v-if|v-else-if|v-else|v-show|v-cloak)$",
    "^(v-once|v-pre)$",
    "^v-for$",
    "^class$",
    "^:class$",
    "^v-bind$",
    "^v-model$",
    ".sync$",
    "$DEFAULT",
    "^(id|ref|key)$",
    "^(v-slot|slot|#)$",
    "^v-",
    "^v-on$",
    "^@",
    "^v-on"
  ],
  "attributeSort": "ASC",
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
